{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:old": "nodemon 'src/index.ts' --exec 'ts-node -r tsconfig-paths/register'",
    "db:migration:generate": "dotenv -e .env.localhost prisma migrate dev --create-only",
    "db:migration:run": "dotenv -e .env.localhost prisma migrate dev",
    "db:reset": "dotenv -e .env.localhost prisma migrate reset",
    "db:pull": "dotenv -e .env.localhost prisma db pull",
    "codegen:graphql": "graphql-codegen --config codegen.yml",
    "codegen:prisma": "dotenv -e .env.localhost prisma generate",
    "prisma-studio": "dotenv -e .env.localhost prisma studio",
    "type-check": "tsc --noEmit",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@envelop/auth0": "4.0.6",
    "@envelop/core": "3.0.6",
    "@envelop/depth-limit": "2.0.6",
    "@envelop/generic-auth": "5.0.6",
    "@envelop/rate-limiter": "4.0.6",
    "@fastify/compress": "6.2.1",
    "@fastify/cors": "8.2.1",
    "@fastify/helmet": "^9.1.0",
    "@graphql-tools/schema": "9.0.16",
    "@graphql-tools/utils": "^9.1.4",
    "@prisma/client": "4.8.1",
    "@types/got": "9.6.12",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "16.18.23",
    "fastify": "4.13.0",
    "got": "11.8.6",
    "graphql": "16.6.0",
    "graphql-tools": "8.3.18",
    "graphql-yoga": "3.4.0",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "2.1.5",
    "prisma": "4.8.1",
    "ts-pattern": "^4.0.5",
    "tsconfig": "workspace:*",
    "typescript": "4.9.5",
    "validation-schema": "workspace:*",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.5",
    "@graphql-codegen/typescript-resolvers": "2.7.13",
    "@graphql-codegen/visitor-plugin-common": "2.13.8",
    "@graphql-yoga/render-graphiql": "3.6.1",
    "chalk": "4.1.2",
    "config": "workspace:*",
    "dotenv-cli": "6.0.0",
    "nodemon": "2.0.20",
    "tsc-alias": "1.8.2",
    "tsconfig-paths": "4.1.2",
    "tsx": "3.12.3"
  }
}
