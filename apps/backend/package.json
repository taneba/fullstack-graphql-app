{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon 'src/index.ts' --exec 'ts-node -r tsconfig-paths/register'",
    "db:migration:generate": "dotenv -e .env.localhost prisma migrate dev --create-only",
    "db:migration:run": "dotenv -e .env.localhost prisma migrate dev",
    "db:reset": "dotenv -e .env.localhost prisma migrate reset",
    "db:pull": "dotenv -e .env.localhost prisma db pull",
    "codegen:graphql": "graphql-codegen --config codegen.yml",
    "codegen:prisma": "dotenv -e .env.localhost prisma generate",
    "prisma-studio": "dotenv -e .env.localhost prisma studio",
    "type-check": "tsc --noEmit",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "lint": "eslint --fix --cache --ext .ts,.tsx src"
  },
  "dependencies": {
    "@envelop/auth0": "3.3.2",
    "@envelop/core": "2.3.1",
    "@envelop/depth-limit": "1.5.1",
    "@envelop/generic-auth": "4.2.1",
    "@envelop/rate-limiter": "^3.0.0",
    "@graphql-tools/schema": "8.3.7",
    "@graphql-yoga/node": "^2.0.0",
    "@prisma/client": "3.5.0",
    "@types/got": "9.6.12",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "16.11.26",
    "fastify": "3.29.0",
    "fastify-compress": "4.0.1",
    "fastify-cors": "6.0.3",
    "fastify-helmet": "7.0.1",
    "got": "11.8.3",
    "graphql": "15.8.0",
    "graphql-tools": "8.2.5",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "2.0.5",
    "prisma": "^3.9.2",
    "ts-pattern": "3.3.5",
    "tsconfig": "*",
    "typescript": "4.5.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript-resolvers": "2.6.4",
    "@graphql-codegen/visitor-plugin-common": "2.8.0",
    "chalk": "4.1.2",
    "config": "*",
    "dotenv-cli": "5.1.0",
    "nodemon": "2.0.15",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "tsc-alias": "1.6.6",
    "tsconfig-paths": "3.12.0"
  }
}
