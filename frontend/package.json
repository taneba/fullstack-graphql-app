{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev & yarn run codegen:watch",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "typecheck": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --watch --config codegen.yml",
    "test": "NODE_ENV=test jest --config ./jest.config.js",
    "test:watch": "NODE_ENV=test jest --watch --config ./jest.config.js",
    "bundle-analyzer": "ANALYZE=true yarn run build"
