    "@radix-ui/react-icons": "1.0.3",
    "@radix-ui/react-portal": "0.1.3",
    "graphql": "15.8.0",
    "graphql-anywhere": "4.2.7",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.22.5",
    "spinners-react": "1.0.6",
    "styled-components": "5.3.3",
