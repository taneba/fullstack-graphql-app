{
  "name": "fullstack-graphql-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "codegen:graphql": "turbo run codegen:graphql",
    "dev:frontend": "turbo run dev --scope=frontend",
    "test:frontend": "turbo run test --scope=frontend",
    "test:watch": "turbo run test:watch",
    "lint:frontend": "turbo run lint --scope=frontend",
    "type-check:frontend": "turbo run type-check --scope=frontend"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "eslint": "^8.9.0",
    "prettier": "^2.5.1",
    "turbo": "1.1.5"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
